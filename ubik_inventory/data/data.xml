<!-- data/ir_sequence_data.xml -->
<odoo>
    <!-- Sequence for Inspection Report -->
    <record id="seq_inspection_report" model="ir.sequence">
        <field name="name">Inspection Report</field>
        <field name="code">inspection.report</field>
        <field name="prefix">QC/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Sequence for Inspection Lines details -->
    <record id="seq_inspection_report_line" model="ir.sequence">
        <field name="name">Inspection Report Line</field>
        <field name="code">inspection.report.line</field>
        <field name="prefix">IL/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
